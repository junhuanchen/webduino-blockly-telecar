<category id="catTelecar">

  <block type="variables_set">
    <field name="VAR" id="=S{3@+J#.JCL.N}p|Fq:" variabletype="">telecar</field>
    <value name="VALUE">
      <block type="telecar_new">
        <field name="address">ws://espressif</field>
      </block>
    </value>
    <next>
      <block type="telecar_move">
        <field name="telecar" id="=S{3@+J#.JCL.N}p|Fq:" variabletype="">telecar</field>
        <field name="move">S</field>
      </block>
    </next>
  </block>
  <block type="telecar_move">
    <field name="telecar" id="=S{3@+J#.JCL.N}p|Fq:" variabletype="">telecar</field>
    <field name="move">N</field>
  </block>
</category>
